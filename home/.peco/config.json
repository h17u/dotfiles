{
  "CustomFilter": {
    "Migemo": {
      "Cmd": "migemogrep",
      "Args": [ "$QUERY" ],
      "BufferThreshold": 100
    }
  },
  "Style": {
    "Basic": ["on_default", "default"],
    "SavedSelection": ["bold", "on_green", "black"],
    "Selected": ["bold", "on_cyan", "black"],
    "Query": ["white", "bold"],
    "Matched": ["black", "on_white"]
  }
}
